#
#fn='max=S=16.90809288548364:SW=0.811199300207978:SS=1.3870308622552656_cfgopts=CFG_SIZE="2*":RF_LUMA="2*":RF=0:SF=3:PS=0:RI=7:R=7:RS=8:SK="sphinx_":WD=0_corruption=VF=scale=540:540_media=images.stats.bz2'
